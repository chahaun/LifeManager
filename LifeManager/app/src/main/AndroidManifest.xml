<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hanmi.lifemanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Custom">
        <activity
            android:name=".Activity.intro_activity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".Activity.Login"
            android:label="@string/login"
            android:screenOrientation="portrait">

        </activity>
        <activity
            android:name=".Activity.SignUp"
            android:label="@string/signUp"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden">

        </activity>

        <activity
            android:name=".Activity.Memo_register"
            android:label="일정 등록"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden">

        </activity>
        <activity
            android:name=".Activity.Time_Calendar"
            android:screenOrientation="portrait"
            android:label="날짜 선택">

        </activity>
        <activity
            android:name=".Activity.Memo_register_Modify"
            android:label="일정 확인 및 수정"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden">

        </activity>
        <activity
            android:name=".Activity.WorkLife_CalendarActivity"
            android:screenOrientation="portrait"
            android:label="일정을 확인하세요!"></activity>

        <activity
            android:name=".Activity.Noti_dialog"
            android:label="Notification"
            android:theme="@android:style/Theme.Dialog"></activity>

        <activity
            android:name=".Activity.Noti_dialog_finish"
            android:label="Notification"
            android:theme="@android:style/Theme.Dialog"></activity>

        <receiver
            android:name=".Receiver.scheduleReceiver"
            android:enabled="true"
            android:exported="true">

        </receiver>
    </application>

</manifest>
